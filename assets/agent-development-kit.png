  <rect x="176" y="180" width="40" height="16" rx="8" ry="8" fill="url(#gradient2)"/>
  
  <!-- Hands/Tools -->
  <circle cx="32" cy="188" r="8" fill="#34A853"/>
  <circle cx="224" cy="188" r="8" fill="#34A853"/>
  
  <!-- Chest panel -->
  <rect x="96" y="192" width="64" height="32" rx="8" ry="8" fill="#fff" opacity="0.3"/>
  
  <!-- Code symbols -->
  <text x="112" y="210" font-family="monospace" font-size="16" fill="#4285F4" font-weight="bold">&lt;/&gt;</text>
  
  <!-- Decorative elements -->
  <circle cx="64" cy="64" r="4" fill="#FBBC04" opacity="0.6"/>
  <circle cx="192" cy="64" r="4" fill="#FBBC04" opacity="0.6"/>
  <circle cx="32" cy="128" r="4" fill="#EA4335" opacity="0.6"/>
  <circle cx="224" cy="128" r="4" fill="#EA4335" opacity="0.6"/>
  <circle cx="64" cy="192" r="4" fill="#34A853" opacity="0.6"/>
  <circle cx="192" cy="192" r="4" fill="#34A853" opacity="0.6"/>
