  </defs>
  
  <!-- Background circle -->
  <circle cx="128" cy="128" r="120" fill="url(#gradient1)" opacity="0.1"/>
  
  <!-- Main robot head -->
  <rect x="64" y="80" width="128" height="96" rx="20" ry="20" fill="url(#gradient1)" stroke="#fff" stroke-width="3"/>
  
  <!-- Eyes -->
