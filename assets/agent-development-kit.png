  <line x1="160" y1="80" x2="160" y2="60" stroke="url(#gradient2)" stroke-width="4" stroke-linecap="round"/>
  <circle cx="96" cy="56" r="6" fill="url(#gradient2)"/>
  <circle cx="160" cy="56" r="6" fill="url(#gradient2)"/>
  
